{
  "name": "easy-text-update",
  "version": "1.0.4",
  "description": "A React utility component to update text easily",
  "author": "Alexandru-Daniel Popescu <ad.popescu338@yahoo.com>",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "dev": "nodemon --watch ./src --ext ts,tsx --exec yarn build"
  },
  "keywords": [
    "react",
    "typescript",
    "update-text",
    "update-text-easily",
    "update-text-react",
    "update-text-react-easily",
    "update-text-react-typescript",
    "update-text-react-typescript-easily",
    "update-text-typescript",
    "update-text-typescript-easily"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.38",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adpopescu338/easy-text-update"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "tslib": "^2.5.0",
    "typescript-plugin-css-modules": "^5.0.1"
  }
}
